
@{
    ViewBag.Title = "GetListIMG";
    ViewBag.ID = "Hình Ảnh";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@using WebAppProwin
@model List<HinhAnh>
    @Html.ActionLink("Trở về","Index","HinhAnh")
    @if (Model.Count > 0)
    {
        foreach(var item in Model)
        {
            <div>
                <img width="200px;" src="~/Upload/@item.path"/>
            </div>
        }
    }

